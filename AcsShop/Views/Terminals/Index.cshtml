@model IEnumerable<AcsShop.Models.TerminalsModel>

@{
    ViewBag.Title = "Index";
}

<h2>Terminals</h2>


@if (Model != null)
{
    foreach (var item in Model)
    {
        <a style="text-decoration:none" href="/Terminal/Terminals/000000000001959">
            <div class="row terminal">
                <div class="col-sm-12">
                    <table>
                        <tr><td> @Html.DisplayNameFor(model => model.Id)</td><td> @Html.DisplayFor(modelItem => item.Id)</td></tr>
                        <tr><td> @Html.DisplayNameFor(model => model.SerialNumber)</td><td> @Html.DisplayFor(modelItem => item.SerialNumber)</td></tr>
                        <tr><td> @Html.DisplayNameFor(model => model.Active)</td><td> @Html.DisplayFor(modelItem => item.Active)</td></tr>
                    </table>
                </div>
            </div>
        </a>
    }
}
else{
    <div class="row">
        <div class="col-sm-offset-4">
            <h4> @ViewBag.Message</h4>
        </div>
    </div>
}


