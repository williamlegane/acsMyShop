@model AcsShop.Models.ProfileModel

@{
    ViewBag.Title = "Index";
}

<h2>Profile</h2>



@if (Model != null)
{
    <div class="row profile">
        <div class="col-sm-12">
            <table>
                <tr>
                    <td>
                        @Html.DisplayNameFor(model => model.ShopName)
                    </td>
                    <td>
                        @Html.DisplayFor(model => model.ShopName)
                    </td>
                </tr>
                <tr>
                    <td>
                        @Html.DisplayNameFor(model => model.MechantNumber)
                    </td>
                    <td>
                        @Html.DisplayFor(model => model.MechantNumber)
                    </td>
                </tr>
                <tr>
                    <td>
                        @Html.DisplayNameFor(model => model.Street)
                    </td>
                    <td>
                        @Html.DisplayFor(model => model.Street)
                    </td>
                </tr>
                <tr>
                    <td>
                        @Html.DisplayNameFor(model => model.City)
                    </td>
                    <td>
                        @Html.DisplayFor(model => model.City)
                    </td>
                </tr>
                <tr>
                    <td>
                        @Html.DisplayNameFor(model => model.RegionCode)
                    </td>
                    <td>
                        @Html.DisplayFor(model => model.RegionCode)
                    </td>
                </tr>
                <tr>
                    <td>
                        @Html.DisplayNameFor(model => model.CountryCode)
                    </td>
                    <td>
                        @Html.DisplayFor(model => model.CountryCode)
                    </td>
                </tr>
            </table>
        </div>
    </div>
}
else
{
    <div class="row">
        <div class="col-sm-offset-4">
            <h4> @ViewBag.Message</h4>
        </div>
    </div>
}



